# E2E Test Environment Variables
# Copy to .env.test and fill in values

# Base URL for the dev server (default: http://localhost:3000)
BASE_URL=http://localhost:3000

# Test user credentials (must exist in the test database)
E2E_TEST_EMAIL=e2e@test.local
E2E_TEST_PASSWORD=TestPassword123!

# Database (test instance)
POSTGRES_URL=postgresql://c1v:dev_password@localhost:5432/c1v_test

# Auth
AUTH_SECRET=test-secret-at-least-32-characters-long

# Anthropic (can use a test/mock key for E2E)
ANTHROPIC_API_KEY=sk-ant-test-key
